{
  "metadata": {
    "blog_title": "Atomic Lab",
    "blog_url": "https://steelydylan.github.io/atomic-lab/",
    "description": "component guide generator based on partial templates"
  },
  "plugins": {
    "metalsmith-collections": {
      "posts": {
        "pattern":"posts/*.md"
      }
    },
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-templates": {
      "engine": "ejs",
      "directory": "templates"
    }
  },
  "source": "./posts",
  "destination": "./docs"
}