<!--@template -->
<%
var title = "Horic Design";
var lead = "a-blog cmsやJavaScriptなどの情報を発信しています。"
var cards = [
  {title:'title'},
  {title: 'title2'},
  {title:'title3'},
  {title: 'title4'},
  {title:'title5'},
  {title: 'title6'}
]; 
%>
<%- include ('../header/header.ejs', {title:"<%= title %>"}) %>
<%- include ('../hero/hero.ejs', {title:"<%= title %>", lead:"<%= lead %>"}) %>
<main class="uc-content content">
  <div class="uc-section">
    <div class="uc-grid _col-2 _small-col-full _gutter">
      <% cards.forEach(function(item) { %>
      <div class="uc-grid-cell">
        <%- include ('../card/card.ejs', {title:"<%= item.title %>"}) %>
      </div> 
      <% }); %>
    </div>
  </div>
</main>
<%- include ('../footer/footer.ejs', {title:"<%= title %>"}) %>
<!--@/template -->

